    },
    {
      "section_id": "para_28",
      "heading": "",
      "content": "enacted  by  the  Parliament  of  Malaysia  as  follows:",
      "section_number": null,
      "subsection": null,
      "page_number": null,
      "element_type": "text"
    },
    {
      "section_id": "para_29",
      "heading": "",
      "content": "short  title",
