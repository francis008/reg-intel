      "element_type": "text"
    },
    {
      "section_id": "para_109",
      "heading": "15.   (1)   Where the Committee is not unanimous on any question or \nmatter to be determined, the decision of the majority shall be deemed \nto be the decision of the Committee.",
      "content": "15.   (1)   Where the Committee is not unanimous on any question or \nmatter to be determined, the decision of the majority shall be deemed \nto be the decision of the Committee.",
      "section_number": null,
      "subsection": null,
      "page_number": null,
