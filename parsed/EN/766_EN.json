    {
      "section_id": "para_66",
      "heading": "1.  (1)  This Act may be cited as the Netting of Financial Agreements \nAct  2015.",
      "content": "1.  (1)  This Act may be cited as the Netting of Financial Agreements \nAct  2015.",
      "section_number": null,
      "subsection": null,
      "page_number": null,
      "element_type": "heading"
    },
    {
      "section_id": "para_67",
      "heading": "",
      "content": "(2)  This  Act  comes  into  operation  on  a  date  to  be  appointed",
      "section_number": null,
      "subsection": null,
      "page_number": null,
      "element_type": "text"
    },
    {
      "section_id": "para_68",
      "heading": "",
      "content": "by  the  Minister  by  notification  in  the  Gazette.",
      "section_number": null,
