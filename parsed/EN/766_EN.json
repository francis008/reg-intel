    },
    {
      "section_id": "para_70",
      "heading": "2.  in  this Act,  unless  the  context  otherwise  requires—",
      "content": "2.  in  this Act,  unless  the  context  otherwise  requires—",
      "section_number": null,
      "subsection": null,
      "page_number": null,
      "element_type": "heading"
    },
    {
      "section_id": "para_71",
      "heading": "",
      "content": "“financial  collateral”  means  an  interest  or  a  right  that  secures \npayment or performance of an obligation under a qualified financial \nagreement;",
      "section_number": null,
      "subsection": null,
      "page_number": null,
      "element_type": "text"
    },
    {
      "section_id": "para_72",
      "heading": "",
      "content": "WJW004137 BI.indd   5",
      "section_number": null,
      "subsection": null,
      "page_number": null,
      "element_type": "text"
    },
    {
      "section_id": "para_73",
      "heading": "3/6/15   4:26:58 PM",
      "content": "3/6/15   4:26:58 PM",
      "section_number": null,
