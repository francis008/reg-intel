    {
      "section_id": "para_41",
      "heading": "WOOD-BASED INDUSTRIES \n(STATE LEGISLATURES \nCOMPETENCY) ACT 1984",
      "content": "WOOD-BASED INDUSTRIES \n(STATE LEGISLATURES \nCOMPETENCY) ACT 1984",
      "section_number": null,
      "subsection": null,
