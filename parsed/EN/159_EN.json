    },
    {
      "section_id": "para_99",
      "heading": "",
      "content": "Offences",
      "section_number": null,
      "subsection": null,
      "page_number": null,
      "element_type": "text"
    },
    {
      "section_id": "para_100",
      "heading": "",
      "content": "Laws of Malaysia",
