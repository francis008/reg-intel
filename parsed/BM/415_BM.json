    },
    {
      "section_id": "para_91",
      "heading": "9.   (1)   Ordinan Perangkaan 1949, dengan ini dimansuhkan.",
      "content": "9.   (1)   Ordinan Perangkaan 1949, dengan ini dimansuhkan.",
      "section_number": null,
      "subsection": null,
