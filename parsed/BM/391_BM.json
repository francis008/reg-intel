    {
      "section_id": "para_30",
      "heading": "",
      "content": "Suatu Akta bagi membolehkan Kerajaan Malaysia menjalankan skim \nperumahan.",
      "section_number": null,
      "subsection": null,
      "page_number": null,
      "element_type": "text"
    },
    {
