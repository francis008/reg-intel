      "content": "A",
      "section_number": null,
      "subsection": null,
      "page_number": null,
      "element_type": "heading"
    },
    {
      "section_id": "para_58",
      "heading": "",
      "content": "(b)  memberi",
      "section_number": null,
      "subsection": null,
      "page_number": null,
      "element_type": "text"
    },
