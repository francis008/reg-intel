      "element_type": "text"
    },
    {
      "section_id": "para_55",
      "heading": "",
      "content": "(c) bagi  500,000  orang  yang  berikutnya  mengikut  kadar",
      "section_number": null,
      "subsection": null,
      "page_number": null,
      "element_type": "text"
    },
    {
