    {
      "section_id": "para_31",
      "heading": "",
      "content": "“Kerajaan” ertinya Kerajaan Malaysia;",
      "section_number": null,
      "subsection": null,
      "page_number": null,
      "element_type": "text"
    },
    {
      "section_id": "para_32",
      "heading": "",
      "content": "“Menteri”  ertinya  Menteri  yang  bertanggungjawab  mengenai",
      "section_number": null,
      "subsection": null,
      "page_number": null,
      "element_type": "text"
    },
    {
      "section_id": "para_33",
      "heading": "",
      "content": "6                              Undang-undang Malaysia                   AKTA 547",
      "section_number": null,
      "subsection": null,
      "page_number": null,
      "element_type": "text"
    },
    {
      "section_id": "para_34",
      "heading": "",
      "content": "pembangunan bandar;",
      "section_number": null,
      "subsection": null,
      "page_number": null,
      "element_type": "text"
    },
    {
      "section_id": "para_35",
      "heading": "",
      "content": "“Perbadanan”  ertinya  Perbadanan  Pembangunan  Bandar  yang \nditubuhkan di bawah Akta Perbadanan Pembangunan Bandar 1971 \n[Akta 46]",
      "section_number": null,
      "subsection": null,
      "page_number": null,
      "element_type": "text"
    },
    {
